{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Deltahack",
  "version": "0.9",
  "manifest_version" : 2,
  "description": "hacks your least favorite math site",
  "options_ui": {
  	"page": "options.html",
  	"open_in_tab": false
  },
  "background" : {
    "scripts" : ["background.js"]
  },
  "browser_action": {
    "default_icon": "logo.png"
  },
  "permissions": [
  	"activeTab",
  	"storage"

  ]

}